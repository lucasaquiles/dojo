    "import numpy as np\n",
    "\n",
    "#exemplo de classificação\n",
    "\n",
    "entradas = np.array([1,7,5])\n",
    "pesos = np.array([0.8,0.1,0])\n",
    "\n",
    "def soma(e,p): \n",
    "    return e.dot(p)\n",
    "\n",
    "s = soma(entradas, pesos)\n",
    "\n",
    "def stepFunction(soma):\n",
    "    if(soma >= 1):\n",
    "        return 1\n",
    "    return 0\n",
    "\n",
    "r = stepFunction(s)\n"
